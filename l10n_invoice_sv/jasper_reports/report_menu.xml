<?xml version="1.0"?>
<odoo>
    <!-- Default Report for FCF -->
    <record id="jasper_fcf_report_demo" model="ir.actions.report">
        <field name="name">FCF Report</field>
        <field name="model">account.invoice</field>
        <field name="string">Jasper FCF Report</field>
        <field name="report_type">jasper</field>
        <field name="file">l10n_invoice_sv/jasper_reports/fcf.jrxml</field>
        <field name="attachment_use">False</field>
        <field name="report_name">Jasper FCF Report</field>
        <field name="jasper_report">True</field>
        <field name="jasper_model_id" ref="account.model_account_invoice"/>
        <field name="binding_model_id" ref="account.model_account_invoice"/>
        <field name="binding_type">report</field>
    </record>

    <record id="jasper_fcf_report_id" model="ir.actions.report.xml.file">
        <field name="default">True</field>
        <field name="filename">fcf.jrxml</field>
        <field name="report_id" ref="l10n_invoice_sv.jasper_fcf_report_demo"/>
        <field name="file" type="base64" file="l10n_invoice_sv/jasper_reports/fcf.jrxml"/>
    </record>

